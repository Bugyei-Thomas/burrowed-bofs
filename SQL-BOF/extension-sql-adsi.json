{
  "name": "sql-adsi",
  "version": "1.0.0",
  "command_name": "sql-adsi",
  "extension_author": "converted for sliver",
  "original_author": "EspressoCake",
  "repo_url": "https://github.com/EspressoCake/SQL-BOF",
  "help": "Obtain ADSI creds from ADSI linked server",
  "long_help": "sql-adsi [server] [ADSI_linkedserver] [opt: port] [opt: database] [opt: linkedserver] [opt: impersonate]. Port defaults to 4444. This gets opened on the SQL Server with the ADSI link, not the beaconing host",
  "depends_on": "coff-loader",
  "entrypoint": "go",
  "files": [
    {
      "os": "windows",
      "arch": "amd64",
      "path": "adsi.x64.o"
    },
    {
      "os": "windows",
      "arch": "386",
      "path": "adsi.x86.o"
    }
  ],
  "arguments": [
    {
      "name": "server",
      "desc": "SQL Server hostname or IP",
      "type": "string",
      "optional": false
    },
    {
      "name": "adsi_server",
      "desc": "ADSI linked server name",
      "type": "string",
      "optional": false
    },
    {
      "name": "port",
      "desc": "Port number (default: 4444)",
      "type": "string",
      "optional": true
    },
    {
      "name": "database",
      "desc": "Database name",
      "type": "string",
      "optional": true
    },
    {
      "name": "linkedserver",
      "desc": "Linked server name",
      "type": "string",
      "optional": true
    },
    {
      "name": "impersonate",
      "desc": "User to impersonate",
      "type": "string",
      "optional": true
    }
  ]
}
