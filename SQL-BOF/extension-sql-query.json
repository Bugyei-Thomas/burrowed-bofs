{
  "name": "sql-query",
  "version": "1.0.0",
  "command_name": "sql-query",
  "extension_author": "converted for sliver",
  "original_author": "EspressoCake",
  "repo_url": "https://github.com/EspressoCake/SQL-BOF",
  "help": "Execute a SQL query on the target server",
  "long_help": "sql-query [server] [query] [opt: database] [opt: linkedserver] [opt: impersonate]",
  "depends_on": "coff-loader",
  "entrypoint": "go",
  "files": [
    {
      "os": "windows",
      "arch": "amd64",
      "path": "query.x64.o"
    },
    {
      "os": "windows",
      "arch": "386",
      "path": "query.x86.o"
    }
  ],
  "arguments": [
    {
      "name": "server",
      "desc": "SQL Server hostname or IP",
      "type": "string",
      "optional": false
    },
    {
      "name": "query",
      "desc": "SQL query to execute",
      "type": "string",
      "optional": false
    },
    {
      "name": "database",
      "desc": "Database name",
      "type": "string",
      "optional": true
    },
    {
      "name": "linkedserver",
      "desc": "Linked server name",
      "type": "string",
      "optional": true
    },
    {
      "name": "impersonate",
      "desc": "User to impersonate",
      "type": "string",
      "optional": true
    }
  ]
}
